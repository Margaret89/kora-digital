extends ../layout/main

block variables
	- var title = 'Content';
	- 
		var pagelist = [
			{href: 'main.html',name:'Main'},
			{href: 'main-sidebar.html',name:'Main with open sidebar'},
			{href: 'main-modal.html',name:'Main with modal'},
			{href: 'blog.html',name:'Blog'},
			{href: 'article.html',name:'Article'},
			{href: 'portfolio.html',name:'Portfolio'},
			{href: 'contacts.html',name:'Contacts'},
			{href: 'typography.html',name:'Typography'},
		];

block content
	section(style="margin-top: 130px;")
		.container
			.row
				.col-12
					h1 Content
					ul
						each page in pagelist
							li
								a(href=page.href,)= page.name


